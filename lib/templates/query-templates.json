{
  "Describe all subjects": {
    "@describe": "?id",
    "@where": {
      "@id": "?id"
    }
  },
  "Construct a subject": {
    "@construct": {
      "@id": "?id",
      "<property>": "?value"
    }
  },
  "Select values of a property": {
    "@select": ["?id", "?value"],
    "@where": {
      "@id": "?id",
      "<property>": "?value"
    }
  }
}
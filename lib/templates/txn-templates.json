{
  "Insert new subject properties": {
    "@id": "<identity>",
    "property1": 1,
    "property2": "<value>"
  },
  "Insert multiple subjects": {
    "@graph": [
      {
        "@id": "<id1>",
        "property1": 1
      },
      {
        "@id": "<id2>",
        "property1": 2
      }
    ]
  },
  "Delete a subject property": {
    "@delete": {
      "@id": "<identity>",
      "property1": 1
    }
  },
  "Delete where a property": {
    "@delete": {
      "@id": "?id",
      "property2": "?any"
    },
    "@where": {
      "@id": "?id",
      "property1": 1,
      "property2": "?any"
    }
  },
  "Update a subject property": {
    "@delete": {
      "@id": "<identity>",
      "property1": 1
    },
    "@insert": {
      "@id": "<identity>",
      "property1": 2
    }
  },
  "Replace all subject properties": {
    "@delete": {
      "@id": "<identity>",
      "?prop": "?value"
    },
    "@insert": {
      "@id": "<identity>",
      "property1": 10,
      "property2": "<new value>"
    }
  },
  "Delete a subject & all references": {
    "@delete": [
      {
        "@id": "<identity>",
        "?prop1": "?value"
      },
      {
        "@id": "?id2",
        "?ref": {
          "@id": "<identity>"
        }
      }
    ],
    "@where": {
      "@union": [
        {
          "@id": "<identity>",
          "?prop1": "?value"
        },
        {
          "@id": "?id2",
          "?ref": {
            "@id": "<identity>"
          }
        }
      ]
    }
  }
}
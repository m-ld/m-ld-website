{
  "name": "m-ld-website",
  "version": "0.0.1",
  "description": "m-ld.org website",
  "scripts": {
    "link-locals": "npm link ../m-ld-js ../m-ld-io-web-base/build ../m-ld-io-web-base/runtime",
    "clean": "rm -rf _site",
    "build": "jest && npx eleventy",
    "local": "now dev",
    "dev": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-ld/m-ld-website.git"
  },
  "author": "George Svarovsky <gsvarovsky@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-ld/m-ld-website/issues"
  },
  "homepage": "https://github.com/m-ld/m-ld-website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@m-ld/io-web-build": "^0.1.3",
    "@types/jest": "^26.0.15",
    "@types/readable-stream": "^2.3.9",
    "bulma": "^0.9.1",
    "jest": "^26.6.3",
    "keypress": "^0.2.1",
    "ts-jest": "^26.4.4",
    "tsify": "^5.0.2",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@m-ld/io-web-runtime": "^0.1.4",
    "@m-ld/m-ld": "^0.5.0",
    "@types/d3": "^5.16.4",
    "@types/jsoneditor": "^8.6.0",
    "@types/level-js": "^4.0.1",
    "@types/local-storage": "^1.4.0",
    "@types/memdown": "^3.0.0",
    "@types/rbush": "^3.0.0",
    "compromise": "^13.7.0",
    "d3": "^5.16.0",
    "jsoneditor": "^9.1.4",
    "level-js": "^5.0.2",
    "line-intersect": "^2.2.4",
    "line2": "^0.2.1",
    "local-storage": "^2.0.0",
    "loglevel": "^1.7.1",
    "memdown": "^5.1.0",
    "rbush": "^3.0.1",
    "rxjs": "^6.6.3",
    "striptags": "^3.1.1",
    "timeago.js": "^4.0.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/lib/",
      "<rootDir>/test/"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "esModuleInterop": true
        }
      }
    }
  }
}

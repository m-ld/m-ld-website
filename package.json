{
  "name": "m-ld-website",
  "version": "0.0.1",
  "description": "m-ld.org website",
  "scripts": {
    "deploy": "npm run build && npm run postcss",
    "build": "node-sass src/main.scss dist/main.css",
    "postcss": "postcss --use autoprefixer --output dist/main.css dist/main.css",
    "watch": "npm run build -- --watch"
